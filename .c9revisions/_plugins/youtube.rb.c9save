{"ts":1381668284340,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"class YouTube < Liquid::Tag\n  Syntax = /^\\s*([^\\s]+)(\\s+(\\d+)\\s+(\\d+)\\s*)?/\n \n  def initialize(tagName, markup, tokens)\n    super\n \n    if markup =~ Syntax then\n      @id = $1\n \n      if $2.nil? then\n          @width = 560\n          @height = 420\n      else\n          @width = $2.to_i\n          @height = $3.to_i\n      end\n    else\n      raise \"No YouTube ID provided in the \\\"youtube\\\" tag\"\n    end\n  end\n \n  def render(context)\n    \"<div class=\\\"yt-container\\\"><iframe class=\\\"yt-embed\\\" width=\\\"#{@width}\\\" height=\\\"#{@height}\\\" src=\\\"http://www.youtube.com/embed/#{@id}\\\"></iframe></div>\"\n  end\n \n  Liquid::Template.register_tag \"youtube\", self\nend"]],"start1":0,"start2":0,"length1":0,"length2":652}]],"length":652}
